package tesr.java;
public class Practice {  //Class
    int id;
    String name;
    public int getId() {
    return id;	
    }
    public void setId(int id) {
    	this.id = id;
    	
    }
    public String getName() {
    	return name;
    }
    public void setName(String name) {
    	this.name = name;
    }
    public static void main(String [] aegs) {
    	Practice student = new Practice(); // Constractor
    	student.setId(1);
    	student.setName("Samir Paul");
    	System.out.println(student.getName() + " is the student with id is " + student.getId());
    }
}
