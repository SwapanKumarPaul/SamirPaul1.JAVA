package tesr.java;
public class Practice {  
	private String name;
	private int age;
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getAge() {
		return age;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public static void main(String[]args) {
		Practice stu = new Practice();
		stu.setName("Samir Paul");
		System.out.println(stu.getName());
		Practice bayas = new Practice();
		bayas.setAge(20);
		System.out.println(bayas.getAge());
		
	}
	
 }
